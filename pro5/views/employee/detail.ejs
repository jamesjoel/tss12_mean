<%- include('../header'); %>
   
<div class="container my-5">
    <div class="row">
        <div class="col-md-6 offset-md-3">
            <h4 class="text-center">Employee Detail</h4>
            <table class="table table-bordered">
                <tr>
                    <td>Full Name</td>
                    <td><%= result.full_name %></td>
                    
                </tr>
                <tr>
                    <td>Email</td>
                    <td><%= result.email %></td>
                
                </tr>
                <tr>
                    <td>Address</td>
                    <td><%= result.address %></td>
                
                </tr>
                <tr>
                    <td>Gender</td>
                    <td><%= result.gender %></td>
                
                </tr>
                <tr>
                    <td>City</td>
                    <td><%= result.city %></td>
                
                </tr>
                <tr>
                    <td>Contact</td>
                    <td><%= result.contact %></td>
                
                </tr>
            </table>
        </div>
    </div>
</div>

<%- include('../footer'); %>
