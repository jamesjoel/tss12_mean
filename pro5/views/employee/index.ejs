<%- include('../header'); %>
    <div class="container my-5">
        <div class="row">
            <div class="col-md-12">
                <h1>Employee Page</h1>
                <div class="row">
                    <div class="col-md-8 offset-md-2">
                        <table class="table table-dark table-hover table-bordered">
                            <tr>
                                <th>S.No.</th>
                                <th>Name</th>
                                <th>Age</th>
                                <th>City</th>
                                <th>Salary</th>
                            </tr>
                            <% emp.forEach(function(x, n){ %>

                                <tr>
                                    <td><%= n+1 %></td>
                                    <td><%= x.name %></td>
                                    <td><%= x.age %></td>
                                    <td><%= x.city %></td>
                                    <td><%= x.salary %></td>
                                </tr>
                                
                            <% }) %>


                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <%- include('../footer'); %>

